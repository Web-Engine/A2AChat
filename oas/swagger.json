  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "Agent": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Agent의 고유 식별자",
            "format": "uuid"
          },
          "name": {
            "type": "string",
            "description": "Agent의 이름"
          },
          "description": {
            "type": "string",
            "description": "Agent에 대한 설명"
          }
        },
        "required": [
          "id",
          "name"
        ]
      },
      "CreateAgentDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Agent의 이름"
          },
          "description": {
            "type": "string",
            "description": "Agent에 대한 설명"
          }
        },
        "required": [
          "name"
        ]
      },
      "Mcp": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "description": "MCP 서버의 URL",
            "format": "uri"
          },
          "token": {
            "type": "string",
            "description": "MCP 서버 인증 토큰"
          },
          "options": {
            "type": "object",
            "description": "추가 설정 옵션",
            "additionalProperties": true
          }
        },
        "required": [
          "url",
          "token"
        ]
      },
      "CreateMcpDto": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "description": "MCP 서버의 URL",
            "format": "uri"
          },
          "token": {
            "type": "string",
            "description": "MCP 서버 인증 토큰"
          },
          "options": {
            "type": "object",
            "description": "추가 설정 옵션",
            "additionalProperties": true
          }
        },
        "required": [
          "url",
          "token"
        ]
      },
      "Schedule": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "예약 실행의 고유 식별자",
            "format": "uuid"
          },
          "agentId": {
            "type": "string",
            "description": "실행할 Agent의 ID",
            "format": "uuid"
          },
          "cronExpression": {
            "type": "string",
            "description": "실행 주기를 나타내는 Cron 표현식"
          },
          "description": {
            "type": "string",
            "description": "예약 실행에 대한 설명"
          },
          "enabled": {
            "type": "boolean",
            "description": "예약 실행 활성화 여부",
            "default": true
          }
        },
        "required": [
          "id",
          "agentId",
          "cronExpression",
          "enabled"
        ]
      },
      "CreateScheduleDto": {
        "type": "object",
        "properties": {
          "agentId": {
            "type": "string",
            "description": "실행할 Agent의 ID",
            "format": "uuid"
          },
          "cronExpression": {
            "type": "string",
            "description": "실행 주기를 나타내는 Cron 표현식"
          },
          "description": {
            "type": "string",
            "description": "예약 실행에 대한 설명"
          },
          "enabled": {
            "type": "boolean",
            "description": "예약 실행 활성화 여부",
            "default": true
          }
        },
        "required": [
          "agentId",
          "cronExpression",
          "enabled"
        ]
      },
      "ChatMessage": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "메시지의 고유 식별자",
            "format": "uuid"
          },
          "senderId": {
            "type": "string",
            "description": "메시지를 보낸 Agent의 ID",
            "format": "uuid"
          },
          "receiverId": {
            "type": "string",
            "description": "메시지를 받는 Agent의 ID",
            "format": "uuid"
          },
          "content": {
            "type": "string",
            "description": "메시지 내용"
          },
          "timestamp": {
            "type": "string",
            "description": "메시지 전송 시간",
            "format": "date-time"
          }
        },
        "required": [
          "id",
          "senderId",
          "receiverId",
          "content",
          "timestamp"
        ]
      },
      "CreateChatMessageDto": {
        "type": "object",
        "properties": {
          "senderId": {
            "type": "string",
            "description": "메시지를 보낸 Agent의 ID",
            "format": "uuid"
          },
          "receiverId": {
            "type": "string",
            "description": "메시지를 받는 Agent의 ID",
            "format": "uuid"
          },
          "content": {
            "type": "string",
            "description": "메시지 내용"
          }
        },
        "required": [
          "senderId",
          "receiverId",
          "content"
        ]
      }
    }
  }
